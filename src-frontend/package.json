{
  "name": "hydra-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "@tauri-apps/api": "^1.2.0",
    "antd": "^4.20.2",
    "clsx": "^1.2.1",
    "craco": "^0.0.3",
    "craco-less": "^2.0.0",
    "create-react-app": "^5.0.1",
    "formik": "^2.2.9",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "immer": "^9.0.19",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^29.4.3",
    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^12.1.5",
    "react-is": "^18.1.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.1",
    "redux-toolkit": "^1.1.2",
    "styled-components": "^5.3.3",
    "tar": "^6.1.11",
    "ts-jest": "^27.1.4",
    "utils-min": "^0.2.1"
  },
  "scripts": {
    "lint": "eslint --ext=.js,.jsx,.tx,.tsx --max-warnings=0 src",
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "test": "react-scripts test --testPathIgnorePatterns=src/__tests__/_helpers --testPathIgnorePatterns=src/__tests__/_data --testPathIgnorePatterns=src/__tests__/_mocks ",
    "eject": "react-scripts eject",
    "prettier": "prettier --check src",
    "format": "prettier --write src"
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios|antd|@ant-design|rc-.+?|@babel/runtime|@tauri-apps.*?|utils-min)).+(js|jsx)$"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0-alpha.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/antd": "^1.0.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.23",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.24",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "less": "^4.1.2",
    "prettier": "^2.8.4",
    "react-scripts": "^5.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^3.1.1"
  },
  "overrides": {
    "craco-less": {
      "@craco/craco": "$@craco/craco"
    }
  }
}
