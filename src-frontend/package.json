    "@tauri-apps/api": "^1.5.3",
    "antd": "^4.24.15",
    "clsx": "^2.1.0",
    "craco": "^0.0.3",
    "craco-less": "^3.0.1",
    "create-react-app": "^5.0.1",
    "formik": "^2.4.5",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "immer": "^10.1.1",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.3.0",
    "react-i18next": "^14.1.1",
    "react-is": "^18.1.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.23.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "redux-toolkit": "^1.1.2",
    "styled-components": "^6.1.11",
    "tar": "^7.1.0",
    "ts-jest": "^27.1.4",
    "utils-min": "^0.2.3"
  },
  "scripts": {
    "lint": "eslint --ext=.js,.jsx,.tx,.tsx --max-warnings=0 src",
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "test": "react-scripts test --testPathIgnorePatterns=src/__tests__/_helpers --testPathIgnorePatterns=src/__tests__/_data --testPathIgnorePatterns=src/__tests__/_mocks ",
    "eject": "react-scripts eject",
    "prettier": "prettier --check src",
    "format": "prettier --write src"
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios|antd|@ant-design|rc-.+?|@babel/runtime|@tauri-apps.*?|utils-min)).+(js|jsx)$"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/antd": "^1.0.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.31",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.34",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "less": "^4.2.0",
    "prettier": "^3.0.3",
    "react-scripts": "^5.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.2"
  },
  "overrides": {
    "craco-less": {
      "@craco/craco": "$@craco/craco"
    }
  }
}
